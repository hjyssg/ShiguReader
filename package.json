{
  "name": "ShiguReader_Backend",
  "version": "2.0.0",
  "description": "All-in-one solution for local doujin/anime/music file",
  "repository": "hjyssg/ShiguReader",
  "main": "src/server/index.js",
  "bin": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "flow": "flow",
    "test": "mocha ./src/test",
    "pkg": "pkg package.json -t host --out-dir executables"
  },
  "pkg": {
    "scripts": "src/server/index.js ",
    "assets": [
      "/dist/*",
      "/node_modules/sqlite3/lib/binding/binding/napi-v6-win32-unknown-x64/*",
      "/node_modules/.store/registry-js@1.15.1/node_modules/registry-js/build/Release/*"
    ],
    "targets": [
      "win-x64"
    ],
    "outputPath": "../SGR_pkg/"
  },
